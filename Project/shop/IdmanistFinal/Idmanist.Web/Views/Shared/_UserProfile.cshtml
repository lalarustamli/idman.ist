<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/Content/css/profilstyle.css">
    <link rel="stylesheet" href="~/Content/css/colors/main.css" id="colors">
    <link rel="stylesheet" href="~/Content/css/icons.css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>
    <div id="wrapper">
        <!-- Header Container
        ================================================== -->
        <header id="header-container" class="fixed fullwidth dashboard">
            <!-- Header -->
            <div id="header" class="not-sticky" style="background: #65d31f;">
                <div class="container">
                    <!-- Left Side Content -->
                    <div class="left-side">
                        <!-- Mobile Navigation -->
                        <!-- Main Navigation / End -->
                    </div>
                    <!-- Left Side Content / End -->
                    <!-- Right Side Content / End -->
                    <div class="right-side">
                        <!-- Header Widget -->
                        <div class="header-widget">
                            <!-- User Menu -->
                            <div class="user-menu">
                                <div class="user-nme">
                                    <a href="@Url.Action("Index", "Home")" style="color:white;">Home page<i class="fa fa-home" aria-hidden="true" style="margin-left:5px; font-size: 20px;"></i></a>
                                    <a href="@Url.Action("index", "user")" style="color:white; margin-left:20px;">Account<i class="fa fa-user-circle-o" aria-hidden="true" style="margin-left:5px; font-size: 20px;"></i></a>
                                </div>
                            </div>
                        </div>
                        <!-- Header Widget / End -->
                    </div>
                    <!-- Right Side Content / End -->
                </div>
            </div>
            <!-- Header / End -->
        </header>
        <div class="clearfix"></div>
        <!-- Header Container / End -->
        <!-- Dashboard -->
        <div id="dashboard">
            <!-- Navigation
            ================================================== -->
            <!-- Responsive Navigation Trigger -->
            <a href="#" class="dashboard-responsive-nav-trigger"><i class="fa fa-reorder"></i> Dashboard Navigation</a>
            <div class="dashboard-nav">
                <div class="dashboard-nav-inner" style="height:450px;">
                    <ul data-submenu-title="Siyahılarım">
                        <li class="active">
                            <a href="@Url.Action("Index", "Liked")"> <i class="fa fa-book" aria-hidden="true"></i>Bəyəndiklərim</a>
                        </li>
                        @*<li><a href="@Url.Action("Index", "Meqale")"><i class="fa fa-pencil-square" aria-hidden="true"></i> Məqalə Göndər</a></li>*@
                    </ul>
                    <ul data-submenu-title="Account">
                        <li class="active"><a href="@Url.Action("Index", "user")"><i class="fa fa-address-book" aria-hidden="true"></i> Profilim</a></li>
                        <li><a href="@Url.Action("Logout", "user")"><i class="fa fa-sign-out" aria-hidden="true"></i> Çıxış</a></li>
                    </ul>
                </div>
            </div>
            <!-- Navigation / End -->
            <!-- Content
            ================================================== -->
            <!-- Content / End -->
            @RenderBody()
        </div>
        <!-- Dashboard / End -->
    </div>
    <!-- Dashboard / End -->
    <!--wrapper ends-->

    <script type="text/javascript" src="~/Content/js/jsscripts/jquery-2.2.0.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jsscripts/jpanelmenu.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jsscripts/chosen.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jsscripts/slick.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jsscripts/rangeslider.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jsscripts/magnific-popup.min.js"></script>
    <script type="text/javascript" src="~/Content/js/waypoints.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jsscripts/counterup.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jsscripts/tooltips.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jsscripts/custom.js"></script>
    <script type="text/javascript" src="~/Content/js/dropzone.js"></script>

</body>
</html>

